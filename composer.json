{
    "name": "lenderspender/laravel-webhook-channel",
    "description": "Send notifications through user defined webhooks",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LenderSpender\\LaravelWebhookChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenderSpender\\LaravelWebhookChannel\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^10.0",
        "spatie/laravel-webhook-server": "^3.1.2"
    },
    "require-dev": {
        "lenderspender/php-cs-fixer-rules": "dev-master",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.3.1",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bert van Hoekelen",
            "email": "bert@magwel.nl"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "LenderSpender\\LaravelWebhookChannel\\LaravelWebhookNotificationServiceProvider"
            ]
        }
    }
}
